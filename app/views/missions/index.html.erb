<div class="container">
  <div class="row">
    <% @missions.each do |mission| %>
      <div class="col-xs-12 col-sm-4">
        <div class="missions-cards">
          <%= svg_div(mission.category) %>
          <h2><%= mission.title.first(26) %></h2>
          <p><%= mission.description.first(65) %>...</p>
        </div>
      </div>
    <% end %>
  </div>
</div>
