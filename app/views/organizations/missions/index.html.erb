<div class="green-background">
</div>
<div class="dashboard-container">
  <div class="asso-namelogo">
    <div class="logo"></div>
    <h3><%= @organization.name %></h3>
  </div>
  <div class="dashboard-btn">
    <div class="btn btn-primary"> Poster un besoin </div>
  </div>
  <div class="dashboard-stat">
    <div class="stat">
      <h2><%= @past.count %> </h2> <p>missions passés</p>
    </div>
    <div class="stat">
      <h2><%= @current.count %></h2> <p>en cours</p>
    </div>
    <div class="stat">
      <h2> <%= @coming.count %></h2><p>à venir</p>
    </div>
  </div>
  <div class="mission-information">
    <p>Besoins en cours</p>
    <% @missions.first(3).each do |mission| %>
    <div class="row">
      <div class="container">
        <div class="row">
          <h4><% mission.title %></h4>
            <div class="col-xs-12 col-sm-4">
              <div class="organizations-cards">
                <div class="organization-looking-for"></div>
                <div class="organization-date">date</div>
                <div class="organization-titre">
                  <h4><%= mission.title %></h4>
                  <div>
                  <a href="#" class="organization-picto"></a>
                  </div>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
    <h10>Besoins passés</h10>
    <% @missions.first(3).each do |mission| %>
    <div class="row">
      <div class="container">
        <div class="row">
          <h4><% mission.title %></h4>
            <div class="col-xs-12 col-sm-4">
              <div class="organizations-cards">
                <div class="organization-looking-for"></div>
                <div class="organization-date">date</div>
                <div class="organization-titre">
                  <h4><%= mission.title %></h4>
                  <div>
                  <a href="#" class="organization-picto"></a>
                  </div>
                </div>
              </div>
            </div>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>

